pipeline{
    agent any
    environment
    {
        PATH="C:\\apache-maven-3.9.6\\bin"
    }
    stages{
        stage('git checkout')
        {
            steps{
                git credentialsId: 'github', url: 'https://github.com/siddireddylakshman/devops-automation.git'
            }
        }
        stage('Maven Build'){
            steps{
                sh 'mvn clean pacakge'
            }
        }        
    }
}
