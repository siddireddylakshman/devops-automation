pipeline{
    agent any
    tools{
        maven 'maven_3_9_6'
    }    
    stages{
        stage('git checkout')
        {
            steps{
                git credentialsId: 'github', url: 'https://github.com/siddireddylakshman/devops-automation.git'
            }
        }
        stage('Maven Build'){
            steps{
                bat 'mvn clean install'
            }
        }        
    }
}
